{
  "name": "manager",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "bun run next build",
    "check": "bun run next lint && bun run tsc --noEmit",
    "db:generate": "bun run drizzle-kit generate",
    "db:start": "docker-compose -f docker-compose.dev.yml up -d",
    "db:stop": "docker-compose -f docker-compose.dev.yml down",
    "db:restart": "docker-compose -f docker-compose.dev.yml down && docker-compose -f docker-compose.dev.yml up -d",
    "db:reset": "docker-compose -f docker-compose.dev.yml down -v",
    "db:migrate": "bun run drizzle-kit migrate",
    "db:push": "bun run drizzle-kit push",
    "db:studio": "bun run drizzle-kit studio",
    "dev": "bun run next dev --turbo",
    "dev:debug": "NODE_OPTIONS='--inspect' bun run next dev --turbo",
    "format:check": "bun run prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "bun run prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "bun run next lint",
    "lint:fix": "bun run next lint --fix",
    "preview": "bun run next build && bun run next start",
    "start": "PORT=4000 bun run next start",
    "typecheck": "bun run tsc --noEmit"
  },
  "dependencies": {
